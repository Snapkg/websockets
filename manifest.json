{
    "schema": 0.2,
    "name": "websockets",
    "version": 0.1,
    "issues": "https://github.com/elfin8er/Snapkg/issues",
    "authors": {
        "maintainers": [
            {
                "username_github": "elfin8er",
                "username_snap": "elfin8er"
            }
        ],
        "developers": [
            {
                "username_github": "elfin8er",
                "username_snap": "elfin8er"
            }
        ]
    },
    "sources": {
        "blocks": [
            {
                "url": "https://raw.githubusercontent.com/elfin8er/Snapkg/development/repositories/default/packages/snapkg/blocks/Snapkg/blocks.xml",
                "provides": [
                    {
                        "spec": "new WebSocket ( %'url' )",
                        "category": "sensing"
                    },
                    {
                        "spec": "%'ws' .send ( %'message' )",
                        "category": "sensing"
                    },
                    {
                        "spec": "%'ws' .on %'event' do %'callback'",
                        "category": "sensing"
                    },
                    {
                        "spec": "%'ws' .close",
                        "category": "sensing"
                    }
                ]
            }
        ]
    }
}